{
    "name": "turnerlabs/validating-xml-encoder",
    "description": "An XML Encoder that validates the result against an XSD.",
    "type": "library",
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/serializer": "~2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Andrew Berry",
            "email": "andrew.berry@lullabot.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "\\TurnerLabs\\ValidatingXmlEncoder\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "\\TurnerLabs\\ValidatingXmlEncoder\\Tests\\" : "tests/src/"
        }
    }
}
